package functions


// Abs returns the absolute value of every number in the slice
// Works for signed numbers
func Abs(type T SignedNumber) (ts []T) ([]T) {
    out := make([]T, len(ts))
    for i,t := range ts {
        if t < 0 {
            t *= -1
        }
        out[i] = t
    }
    return out
}
