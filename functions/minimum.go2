package functions

// Minimum returns the lowest number in the slice
// Works for any number type
func Minimum(type T Number) (ts []T) T {
    if len(ts) == 0 {
        return 0 
    }
    min := ts[0]
    for _,t := range ts[1:] {
        if t < min {
            min = t
        }
    }
    return min 
}
