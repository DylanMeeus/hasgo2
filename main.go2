package main

import (
	"fmt"
)

func Filter(type T)(ts []T, f func(T) bool) (out []T) {
	for _, t := range ts {
		if f(t) {
			out = append(out, t)
		}
	}
	return out
}

// testing out some hasgo2 code
func main() {
	fmt.Println("hello world")
	is := []int{1, 2, 3, 4}
	f := Filter(is, func(i int) bool { return i%2 == 0 })
        fmt.Println(f)
}
